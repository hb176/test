<?xml version="1.0" encoding="UTF-8" ?>   
<!DOCTYPE mapper   
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"  
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd"> 
<mapper namespace="com.example.dao.MenuMapper">
	<resultMap type="com.example.dto.Menu" id="menuResultMap">
		<id column="MENU_ID" property="menuId"/>
		<result column="MENU_NAME" property="menuName"/>
		<result column="MENU_URL" property="menuUrl"/>
		<result column="MENU_LEVEL" property="menuLevel"/>
		<result column="MENU_PARENT_ID" property="menuParentId"/>
		<result column="MENU_ORDER" property="menuOrder"/>
		<result column="MENU_CREATE_TIME" property="menuCreateTime"/>
		<!-- <collection property="role" javaType="com.union.entity.Role" resultMap="roleResultMap"></collection> -->
	</resultMap>

</mapper> 